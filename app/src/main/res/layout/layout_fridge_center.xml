<merge xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#85C246"
        tools:ignore="ExtraText">


        ///!**********************Top Toolbar**********************

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="-15dp"
            android:layout_marginTop="-88dp"
            android:contentDescription="@string/background"
            app:srcCompat="@drawable/bc" />

        <RelativeLayout
            android:id="@+id/relllayout1"
            android:layout_width="match_parent"
            android:layout_height="50dp">

            <include layout="@layout/fridge_bar" />


        </RelativeLayout>
        ///!**********************Middle Section**********************

        <RelativeLayout
            android:id="@+id/relllayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relllayout1"
            android:background="@drawable/black_border" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="411dp"
            android:layout_height="match_parent"
            android:layout_alignStart="@+id/imageView12"
            android:layout_alignLeft="@+id/imageView12"
            android:layout_alignTop="@+id/relllayout1"
            android:layout_alignEnd="@+id/imageView12"
            android:layout_alignRight="@+id/imageView12"
            android:layout_alignBottom="@+id/imageView12"
            android:layout_marginStart="-3dp"
            android:layout_marginLeft="-3dp"
            android:layout_marginTop="2dp"
            android:layout_marginEnd="3dp"
            android:layout_marginRight="3dp"
            android:layout_marginBottom="-2dp"
            android:contentDescription="@string/fridge"
            android:src="@drawable/fridgelarger" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/create"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView10"
            android:layout_alignRight="@+id/imageView10"
            android:layout_alignBottom="@+id/imageView10"
            android:layout_marginEnd="26dp"
            android:layout_marginRight="26dp"
            android:layout_marginBottom="273dp"
            android:clickable="true"
            android:focusable="true"
            android:src="@drawable/pencilnew"
            app:backgroundTint="#1E88E5"
            app:maxImageSize="50dp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/button_scan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/expirationdate"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="290dp"
            android:layout_marginRight="290dp"
            android:layout_marginBottom="-143dp"
            android:clickable="true"
            android:focusable="true"
            app:backgroundTint="#00ACC1"
            app:maxImageSize="50dp"
            app:srcCompat="@drawable/barcode" />

        <TextView
            android:id="@+id/result_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView12"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginEnd="285dp"
            android:layout_marginRight="285dp"
            android:layout_marginBottom="235dp"
            android:gravity="center"
            android:text="Result"
            android:textSize="22sp" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/expirationdate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView12"
            android:layout_marginEnd="-59dp"
            android:layout_marginRight="-59dp"
            android:layout_marginBottom="361dp"
            android:layout_toStartOf="@+id/create"
            android:layout_toLeftOf="@+id/create"
            android:clickable="true"
            android:focusable="true"
            app:backgroundTint="#D81B60"
            app:maxImageSize="53dp"
            app:rippleColor="#F4511E"
            app:srcCompat="@drawable/calendar" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fridgeinventory"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView10"
            android:layout_alignRight="@+id/imageView10"
            android:layout_alignBottom="@+id/imageView12"
            android:layout_marginEnd="148dp"
            android:layout_marginRight="148dp"
            android:layout_marginBottom="190dp"
            android:clickable="true"
            android:focusable="true"
            app:backgroundTint="#3DB944"
            app:maxImageSize="47dp"
            app:srcCompat="@drawable/fridgelibrary" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/addproduct"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageView12"
            android:layout_alignRight="@+id/imageView12"
            android:layout_alignBottom="@+id/imageView12"
            android:layout_marginEnd="291dp"
            android:layout_marginRight="291dp"
            android:layout_marginBottom="357dp"
            android:clickable="true"
            android:focusable="true"
            app:backgroundTint="#5E35B1"
            app:maxImageSize="42dp"
            app:srcCompat="@drawable/add" />


    </RelativeLayout>



</merge>